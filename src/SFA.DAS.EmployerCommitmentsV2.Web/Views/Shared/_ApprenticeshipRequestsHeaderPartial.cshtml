@model SFA.DAS.EmployerCommitmentsV2.Web.Models.Cohort.CohortsViewModel

<div class="govuk-grid-row">
    <div class="govuk-grid-column-">
        <div class="govuk-width-container">
            <h1 class="govuk-heading-xl">Apprentice requests</h1>

            <a asp-controller="Cohort" asp-action="Index" class="govuk-button"> Add new apprentice </a>

            <div class="das-tabs-boxes">
                <ul class="das-tabs-boxes__list">
                    @await Html.PartialAsync("_BingoBoxTabItem", Model.CohortsInReview)
                    @await Html.PartialAsync("_BingoBoxTabItem", Model.CohortsWithTrainingProvider)
                    @await Html.PartialAsync("_BingoBoxTabItem", Model.CohortsInDraft)
                    @await Html.PartialAsync("_BingoBoxTabItem", Model.CohortsWithTransferSender)
                </ul>
            </div>
        </div>
    </div>
</div>